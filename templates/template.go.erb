package <%= package %>

<% if imports.any? %>
import (
  <% imports.each do |i| %>
    "<%= i %>"
  <% end %>
)
<% end %>

type <%= @name %> struct {
  <% @properties.each do |p| %>
    <%= p.name.camelize %> <%= p.go_type %> `json:"<%= p.name %>"`
  <% end %>

  <% if @actions.any? %>
    api *API
  <% end %>
}

<% @actions.each do |action| %>
<%= action.render %>
<% end %>

